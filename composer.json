{
    "name": "masnathan/domain-api-client",
    "description": "This is an easy way to check domain details.",
    "keywords": [
        "masnathan",
        "domain",
        "whois",
        "dns"
    ],
    "homepage": "https://github.com/masnathan/domain-api-client",
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.3",
        "ext-json": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.3",
        "vimeo/psalm": "^4.9"
    },
    "autoload": {
        "psr-4": {
            "MASNathan\\DomainAPI\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MASNathan\\DomainAPI\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes src/"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
